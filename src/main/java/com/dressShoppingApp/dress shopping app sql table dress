DROP TABLE Dressitems;

CREATE TABLE Dressitems (
DressIdNo INT PRIMARY KEY AUTO_INCREMENT,
DressName VARCHAR ( 100 ) NOT NULL,
BrandName VARCHAR ( 100 ) NOT NULL,
Colour VARCHAR ( 100 ) NOT NULL,
Price BIGINT ( 6 ) NOT NULL
);

SELECT * FROM Dressitems;

