DROP TABLE dress_shopping_app_orders;

CREATE TABLE dress_shopping_app_orders (
order_id INT PRIMARY KEY AUTO_INCREMENT,
id INT NOT NULL,
size VARCHAR ( 100 ) NOT NULL,
price BIGINT ( 6 ) NOT NULL,
COUNT BIGINT ( 10 ) NOT NULL,
total_price BIGINT ( 10 ) NOT NULL
);

SELECT * FROM dress_shopping_app_orders;